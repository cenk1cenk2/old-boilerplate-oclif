{
  "name": "@cenk1cenk2/boilerplate-oclif",
  "description": "Base custom boilerplate for oclif.",
  "version": "1.0.0",
  "author": "Cenk Kilic <cenk@kilic.dev>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/cenk1cenk2/boilerplate-oclif",
  "scripts": {
    "prebuild": "rimraf tsconfig*.tsbuildinfo && rimraf dist/",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "tsconfig-replace-paths --project tsconfig.json",
    "dev:build": "yarn prebuild && tsc -p tsconfig.json && yarn postbuild",
    "dev:start": "yarn dev:build && tsc-watch --noClear --onSuccess \"yarn postbuild\"",
    "lint": "prettier --write src/ --loglevel warn && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged"
  },
  "publishConfig": {
    "access": "public"
  },
  "simple-git-hooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "./*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "./*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "config": "^3.3.6",
    "deepmerge": "^4.2.2",
    "figures": "^3.2.0",
    "object-path-immutable": "^4.1.2",
    "table": "^6.7.3",
    "tslib": "^2.3.1",
    "types": "^0.1.1",
    "winston": "^3.3.3",
    "yaml": "^1.10.2"
  },
  "peerDependencies": {
    "@oclif/command": ">= 1.7.0 < 2",
    "@oclif/config": ">= 1.16.0 < 2",
    "@oclif/errors": ">= 1.3.0 < 2",
    "execa": ">= 4.0.0",
    "fs-extra": ">= 9.0.0",
    "globby": ">= 11.0.0",
    "listr2": ">= 2.5.0"
  },
  "devDependencies": {
    "@cenk1cenk2/eslint-config": "^1.0.8",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/errors": "^1.3.5",
    "@types/config": "^0.0.40",
    "@types/figures": "^3.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/object-path": "^0.11.1",
    "eslint": "^8.2.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "globby": "^12.0.2",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "simple-git-hooks": "^2.7.0",
    "listr2": "^3.13.3",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-replace-paths": "^0.0.11",
    "typescript": "^4.4.4"
  }
}
